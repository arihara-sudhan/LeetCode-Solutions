import java.util.*;
class Main{
    public static void main(String args[])
    {
        int prices[]={7,1,5,3,6,4};
        int buy=prices[0];
        int sell=0;
        int profit=0;
        for(int i=1;i<prices.length;i++)
        {
            if(prices[i-1]<prices[i])
            {
                profit+=prices[i]-prices[i-1];
            }
           
        }    
        System.out.print(profit);
    }
}
