import java.util.*;
class Main{
    public static void main(String args[])
    {
        int ar[]={1,2,3,4,5,6,7};
        int target=7;
        for(int i=0;i<ar.length-1;i++)
        {
            for(int j=i+1;j<ar.length;j++)
            {
                if(ar[i]+ar[j]==target)
                {
                    System.out.println(+i +"," +j);
                    break;
                }
            }
        }
        
        
    }
}
